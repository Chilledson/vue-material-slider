{
  "name": "vue-material-slider",
  "version": "0.1.1",
  "main": "./dist/lib/vue-material-slider.common.js",
  "scripts": {
    "serve": "vue-cli-service serve src/components/slider.vue",
    "build": "npm run build:wc && npm run build:lib && npm run build:app",
    "build:wc": "vue-cli-service build src/plugin.js --target wc --name vue-material-slider --dest dist/wc",
    "build:lib": "vue-cli-service build src/plugin.js --target lib --name vue-material-slider --dest dist/lib",
    "build:app": "vue-cli-service build src/plugin.js --target app --name vue-material-slider --dest dist/app",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "^2.5.17"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
